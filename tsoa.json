{
  "entryFile": "src/app.ts",
  "noImplicitAdditionalProperties": "throw-on-extras",
  "controllerPathGlobs": ["src/**/*Controller.ts"],
  "spec": {
    "outputDirectory": "build",
    "specVersion": 3,
    "securityDefinitions": {
      "session": {
        "type": "session",
        "name": "session"
      }
    }
  },
  "routes": {
    "authenticationModule": "src/authentication.ts",
    "routesDir": "build"
  }
}